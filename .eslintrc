{
  "extends": [
    "eslint:recommended",
    "plugin:jest/recommended",
    "plugin:react/recommended",
    "prettier"
  ],
  "env": {
    "react-native/react-native": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
  },
  "plugins": [
    "jest",
    "prettier",
    "react",
    "react-native"
  ],
  "rules": {
    "prettier/prettier": "error",
    "react/no-unused-prop-types": "error"
  },
  "globals": {
    "Expo": true
  }
}
